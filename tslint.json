{
	"extends": [
		"tslint:recommended",
		"tslint-config-prettier"
	],
	"rules": {
		"max-classes-per-file": [
			true,
			5
		],
		"interface-over-type-literal": false,
		"variable-name": [
			true,
			"check-format",
			"allow-leading-underscore"
		],
		"ordered-imports": [
			false,
			{
				"named-imports-order": "any"
			}
		]
	}
}
